{
  "name": "mnemonic-id",
  "description": "Library to generate easy to remember and sometimes entertaining human readable ids",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Mattias E. O. Andersson",
  "repository": "github:Adelost/nodejs-typescript-boilerplate",
  "homepage": "https://github.com/Adelost/mnemonic-id#readme",
  "keywords": [
    "id",
    "uuid",
    "mnemonic",
    "random"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist dist-minified coverage tmp",
    "start": "node dist/app.js",
    "start-minified": "node dist-minified/app.js",
    "build": "tsc",
    "watch": "tsc -w",
    "minify": "webpack --display-modules",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier ./src --write",
    "lint": "eslint --ext ts ./src",
    "lint-fix": "npm run format && npm run lint -- --fix"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "eslint": "7.1.0",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "typescript": "3.9.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {
    "tslib": "2.0.0"
  }
}
